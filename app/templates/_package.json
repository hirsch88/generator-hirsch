{
  "name":            "<%= prompts.appname %>",
  "version":         "0.0.0",
  "description":     "<%= prompts.description %>",
  "prefix":          "<%= prompts.prefix %>",
  "main":            "<%= path.distDir %>/<%= path.main %>",
  "cssExtension":    "<%= prompts.cssExtension %>",
  "language":        <% if(prompts.useTypescript) { %>"TypeScript"<% } else { %>"JavaScript"<% } %>,
  "private":         true,
  "dependencies":    {},
  "devDependencies": {
    "gulp":                   "^3.8.11",
    "gulp-concat":            "^2.5.2",
    "gulp-autoprefixer":      "^2.3.1",
    "gulp-install":           "^0.2.0",
    "gulp-header":            "^1.2.2",
    "gulp-htmlmin":           "^1.1.1",
    "gulp-inject":            "^1.2.0",
    "gulp-jshint":            "^1.11.2",
    "gulp-htmllint":          "*",
    "gulp-bootlint":          "*",
    "gulp-karma":             "0.0.4",<% if (prompts.useLess) { %>
    "gulp-less":              "^3.0.1",<% } %><% if (prompts.useSass) { %>
    "gulp-sass":              "*",
    "node-sass":              "^2.0.4",<% } %>
    "gulp-load-plugins":      "^0.8.1",
    "gulp-minify-css":        "^1.0.0",
    "gulp-minify-html":       "^0.1.7",
    "gulp-ng-annotate":       "^0.5.2",
    "gulp-rename":            "^1.2.0",
    "gulp-template":          "^3.0.0",
    "gulp-sourcemaps":        "^1.5.2",
    "gulp-task-listing":      "^1.0.0",
    "gulp-uglify":            "^1.1.0",
    "gulp-util":              "^3.0.4",
    "gulp-watch":             "^4.1.1",
    "require-dir":            "*",
    "del":                    "^1.1.1",
    "browser-sync":           "^2.2.3",
    "chalk":                  "^1.0.0",
    "mocha":                  "^2.1.0",
    "chai":                   "^2.0.0",
    "karma":                  "*",
    "karma-chai":             "^0.1.0",
    "karma-chrome-launcher":  "^0.1.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha":            "^0.1.10",
    "karma-ng-scenario":      "^0.1.0",
    "jshint-stylish":         "^1.0.1",
    "lodash":                 "^3.5.0",
    "bower-files":            "^3.3.0",
    "main-bower-files":       "^2.5.0",
    "wiredep":                "^2.2.2"<% if (prompts.useTypescript) { %>,
    "tsd": "^0.6.0",
    "typescript": "~1.4.0",
    "gulp-typescript": "^2.7.5",
    "gulp-tslint": "^2.0.0"<% } %>
  },
  "scripts":         {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository":      {
    "type": "git",
    "url":  ""
  },
  "keywords":        [
    "AngularJS"
  ],
  "author":          "<%= prompts.author %>",
  "license":         "MIT",
  "bugs":            {
    "url": ""
  },
  "homepage":        ""
}
